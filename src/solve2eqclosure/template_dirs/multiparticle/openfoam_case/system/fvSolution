FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       dictionary;
    location    "system/particle";
    object      fvSolution;
}

solvers
{
    h
    {
    solver PCG;
    preconditioner DIC;
    tolerance	1e-8;
    }

    hFinal
    {
        $h;
        relTol           0;
    }
}

PIMPLE
{
    nNonOrthogonalCorrectors 0;
}
